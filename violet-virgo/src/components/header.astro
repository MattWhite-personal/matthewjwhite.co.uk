---
import Navigation from "./navigation.astro";
import Hamburger from "./hamburger.astro";
const { pageTitle } = Astro.props;
---

<header>
    <nav>
        <Hamburger />
        <div class="nav-links">
            <Navigation url="/" title="Home" />
            <Navigation url="/about" title="About Me" />
            <Navigation url="/blog" title="Blog" />
        </div>
    </nav>
</header>
<h1>{pageTitle}</h1>
